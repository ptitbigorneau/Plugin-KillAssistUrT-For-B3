<configuration plugin="killassisturt">
    <settings name="settings">
<!--plugin actived on/off-->
        <set name="pluginactived">on</set>
<!--min level required to use the assist command -->
		<set name="assistlevel">1</set>
<!--min level required for enable / disable plugin -->
		<set name="adminlevel">100</set>
<!--assist time delay -->
		<set name="assistdelay">10</set>
        </settings>
</configuration>

